<!DOCTYPE html>
<html>
<head>
  <title>Semantic Themes</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= yield :header %>
</head>
<body class="<%= controller_name %>-<%= action_name %>">
  <div class="sticky-footer-wrapper">
    <%= render 'shared/nav' %>
    <%= yield %>
    <div class="push"></div>
  </div>
  <%= render 'shared/footer' %>

</body>
</html>
